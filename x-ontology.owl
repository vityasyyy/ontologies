<?xml version="1.0" encoding="UTF-8"?>
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xmlns:dc="http://purl.org/dc/elements/1.1/"
         xmlns:foaf="http://xmlns.com/foaf/0.1/">

<!-- 
=================================================================
Ontology Metadata
=================================================================
-->
  <owl:Ontology rdf:about="https://vityasyyy.github.io/ontologies/x-ontology.owl">
    <rdfs:label>Twitter Social Media Ontology</rdfs:label>
    <rdfs:comment>An ontology for modeling Twitter social media interactions, users, content, and relationships. This ontology focuses on core Twitter entities including users, tweets, hashtags, media content, and social interactions like following, liking, and retweeting.</rdfs:comment>
    <owl:versionInfo>2.0</owl:versionInfo>
    <dc:creator>Twitter Ontology Development Team</dc:creator>
    <dc:date>2025-09-25</dc:date>
    <dc:description>Competency questions: Who follows whom? What tweets contain specific hashtags? Which users have posted replies to a tweet? What media is attached to tweets?</dc:description>
</owl:Ontology>

<!-- 
=================================================================
Top-level Classes
=================================================================
-->

<owl:Class rdf:about="#TwitterEntity">
    <rdfs:label>Twitter Entity</rdfs:label>
    <rdfs:comment>Abstract superclass for all Twitter-related entities</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="#Agent">
    <rdfs:label>Agent</rdfs:label>
    <rdfs:comment>An entity capable of performing actions on Twitter</rdfs:comment>
    <rdfs:subClassOf rdf:resource="#TwitterEntity"/>
</owl:Class>

<owl:Class rdf:about="#Content">
    <rdfs:label>Content</rdfs:label>
    <rdfs:comment>Textual or media content shared on Twitter</rdfs:comment>
    <rdfs:subClassOf rdf:resource="#TwitterEntity"/>
</owl:Class>

<!-- 
=================================================================
User Classes
=================================================================
-->

<owl:Class rdf:about="#User">
    <rdfs:label>User</rdfs:label>
    <rdfs:comment>A Twitter user account</rdfs:comment>
    <rdfs:subClassOf rdf:resource="#Agent"/>
    <rdfs:subClassOf rdf:resource="http://xmlns.com/foaf/0.1/Person"/>
</owl:Class>

<owl:Class rdf:about="#RegularUser">
    <rdfs:label>Regular User</rdfs:label>
    <rdfs:comment>A standard Twitter user without verification</rdfs:comment>
    <rdfs:subClassOf rdf:resource="#User"/>
</owl:Class>

<owl:Class rdf:about="#VerifiedUser">
    <rdfs:label>Verified User</rdfs:label>
    <rdfs:comment>A Twitter user with a verified badge</rdfs:comment>
    <rdfs:subClassOf rdf:resource="#User"/>
</owl:Class>

<owl:Class rdf:about="#BusinessAccount">
    <rdfs:label>Business Account</rdfs:label>
    <rdfs:comment>A Twitter account representing a business or organization</rdfs:comment>
    <rdfs:subClassOf rdf:resource="#User"/>
</owl:Class>

<!-- 
=================================================================
Tweet Classes
=================================================================
-->

<owl:Class rdf:about="#Tweet">
    <rdfs:label>Tweet</rdfs:label>
    <rdfs:comment>A message posted on Twitter</rdfs:comment>
    <rdfs:subClassOf rdf:resource="#Content"/>
</owl:Class>

<owl:Class rdf:about="#Reply">
    <rdfs:label>Reply</rdfs:label>
    <rdfs:comment>A tweet posted in response to another tweet</rdfs:comment>
    <rdfs:subClassOf rdf:resource="#Tweet"/>
</owl:Class>

<owl:Class rdf:about="#Retweet">
    <rdfs:label>Retweet</rdfs:label>
    <rdfs:comment>A tweet that shares another user's tweet</rdfs:comment>
    <rdfs:subClassOf rdf:resource="#Tweet"/>
</owl:Class>

<owl:Class rdf:about="#QuoteTweet">
    <rdfs:label>Quote Tweet</rdfs:label>
    <rdfs:comment>A tweet that quotes another tweet with additional commentary</rdfs:comment>
    <rdfs:subClassOf rdf:resource="#Tweet"/>
</owl:Class>

<!-- 
=================================================================
Media Classes
=================================================================
-->

<owl:Class rdf:about="#Media">
    <rdfs:label>Media</rdfs:label>
    <rdfs:comment>Media content attached to tweets</rdfs:comment>
    <rdfs:subClassOf rdf:resource="#Content"/>
</owl:Class>

<owl:Class rdf:about="#Image">
    <rdfs:label>Image</rdfs:label>
    <rdfs:comment>Image media content including photos, graphics, and animations</rdfs:comment>
    <rdfs:subClassOf rdf:resource="#Media"/>
</owl:Class>

<owl:Class rdf:about="#Video">
    <rdfs:label>Video</rdfs:label>
    <rdfs:comment>Video media content</rdfs:comment>
    <rdfs:subClassOf rdf:resource="#Media"/>
    <owl:disjointWith rdf:resource="#Image"/>
</owl:Class>

<owl:Class rdf:about="#Audio">
    <rdfs:label>Audio</rdfs:label>
    <rdfs:comment>Audio media content</rdfs:comment>
    <rdfs:subClassOf rdf:resource="#Media"/>
    <owl:disjointWith rdf:resource="#Image"/>
    <owl:disjointWith rdf:resource="#Video"/>
</owl:Class>

<!-- 
=================================================================
Hashtag Classes
=================================================================
-->

<owl:Class rdf:about="#Hashtag">
    <rdfs:label>Hashtag</rdfs:label>
    <rdfs:comment>A hashtag used for categorizing tweets</rdfs:comment>
    <rdfs:subClassOf rdf:resource="#TwitterEntity"/>
</owl:Class>

<owl:Class rdf:about="#TrendingHashtag">
    <rdfs:label>Trending Hashtag</rdfs:label>
    <rdfs:comment>A hashtag that is currently trending</rdfs:comment>
    <rdfs:subClassOf rdf:resource="#Hashtag"/>
</owl:Class>

<!-- 
=================================================================
Timeline Classes
=================================================================
-->

<owl:Class rdf:about="#Timeline">
    <rdfs:label>Timeline</rdfs:label>
    <rdfs:comment>A chronological display of tweets</rdfs:comment>
    <rdfs:subClassOf rdf:resource="#TwitterEntity"/>
</owl:Class>

<owl:Class rdf:about="#UserTimeline">
    <rdfs:label>User Timeline</rdfs:label>
    <rdfs:comment>A timeline showing a specific user's tweets</rdfs:comment>
    <rdfs:subClassOf rdf:resource="#Timeline"/>
</owl:Class>

<owl:Class rdf:about="#HomeTimeline">
    <rdfs:label>Home Timeline</rdfs:label>
    <rdfs:comment>A user's personalized timeline with tweets from followed users</rdfs:comment>
    <rdfs:subClassOf rdf:resource="#Timeline"/>
</owl:Class>

<!-- 
=================================================================
Object Properties - Relationships
=================================================================
-->

<owl:ObjectProperty rdf:about="#follows">
    <rdfs:subPropertyOf rdf:resource="http://xmlns.com/foaf/0.1/knows"/>
    <rdfs:label>follows</rdfs:label>
    <rdfs:comment>A user follows another user</rdfs:comment>
    <rdfs:domain rdf:resource="#User"/>
    <rdfs:range rdf:resource="#User"/>
    <owl:inverseOf rdf:resource="#followedBy"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#followedBy">
    <rdfs:label>followed by</rdfs:label>
    <rdfs:comment>A user is followed by another user</rdfs:comment>
    <rdfs:domain rdf:resource="#User"/>
    <rdfs:range rdf:resource="#User"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#posts">
    <rdfs:label>posts</rdfs:label>
    <rdfs:comment>A user posts a tweet</rdfs:comment>
    <rdfs:domain rdf:resource="#User"/>
    <rdfs:range rdf:resource="#Tweet"/>
    <owl:inverseOf rdf:resource="#postedBy"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#postedBy">
    <rdfs:label>posted by</rdfs:label>
    <rdfs:comment>A tweet is posted by a user</rdfs:comment>
    <rdfs:domain rdf:resource="#Tweet"/>
    <rdfs:range rdf:resource="#User"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#retweets">
    <rdfs:label>retweets</rdfs:label>
    <rdfs:comment>A retweet references the original tweet</rdfs:comment>
    <rdfs:domain rdf:resource="#Retweet"/>
    <rdfs:range rdf:resource="#Tweet"/>
    <owl:inverseOf rdf:resource="#retweetedBy"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#retweetedBy">
    <rdfs:label>retweeted by</rdfs:label>
    <rdfs:comment>A tweet is retweeted by a retweet</rdfs:comment>
    <rdfs:domain rdf:resource="#Tweet"/>
    <rdfs:range rdf:resource="#Retweet"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#quotes">
    <rdfs:label>quotes</rdfs:label>
    <rdfs:comment>A quote tweet references another tweet while adding commentary</rdfs:comment>
    <rdfs:domain rdf:resource="#QuoteTweet"/>
    <rdfs:range rdf:resource="#Tweet"/>
    <owl:inverseOf rdf:resource="#quotedBy"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#quotedBy">
    <rdfs:label>quoted by</rdfs:label>
    <rdfs:comment>A tweet is quoted by a quote tweet</rdfs:comment>
    <rdfs:domain rdf:resource="#Tweet"/>
    <rdfs:range rdf:resource="#QuoteTweet"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#repliesTo">
    <rdfs:label>replies to</rdfs:label>
    <rdfs:comment>A reply tweet responds to another tweet</rdfs:comment>
    <rdfs:domain rdf:resource="#Reply"/>
    <rdfs:range rdf:resource="#Tweet"/>
    <owl:inverseOf rdf:resource="#hasReply"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#hasReply">
    <rdfs:label>has reply</rdfs:label>
    <rdfs:comment>A tweet has a reply</rdfs:comment>
    <rdfs:domain rdf:resource="#Tweet"/>
    <rdfs:range rdf:resource="#Reply"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#likes">
    <rdfs:label>likes</rdfs:label>
    <rdfs:comment>A user likes a tweet</rdfs:comment>
    <rdfs:domain rdf:resource="#User"/>
    <rdfs:range rdf:resource="#Tweet"/>
    <owl:inverseOf rdf:resource="#likedBy"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#likedBy">
    <rdfs:label>liked by</rdfs:label>
    <rdfs:comment>A tweet is liked by a user</rdfs:comment>
    <rdfs:domain rdf:resource="#Tweet"/>
    <rdfs:range rdf:resource="#User"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#mentions">
    <rdfs:label>mentions</rdfs:label>
    <rdfs:comment>A tweet mentions a user</rdfs:comment>
    <rdfs:domain rdf:resource="#Tweet"/>
    <rdfs:range rdf:resource="#User"/>
    <owl:inverseOf rdf:resource="#mentionedIn"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#mentionedIn">
    <rdfs:label>mentioned in</rdfs:label>
    <rdfs:comment>A user is mentioned in a tweet</rdfs:comment>
    <rdfs:domain rdf:resource="#User"/>
    <rdfs:range rdf:resource="#Tweet"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#containsHashtag">
    <rdfs:label>contains hashtag</rdfs:label>
    <rdfs:comment>A tweet contains a hashtag</rdfs:comment>
    <rdfs:domain rdf:resource="#Tweet"/>
    <rdfs:range rdf:resource="#Hashtag"/>
    <owl:inverseOf rdf:resource="#usedIn"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#usedIn">
    <rdfs:label>used in</rdfs:label>
    <rdfs:comment>A hashtag is used in a tweet</rdfs:comment>
    <rdfs:domain rdf:resource="#Hashtag"/>
    <rdfs:range rdf:resource="#Tweet"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#containsMedia">
    <rdfs:label>contains media</rdfs:label>
    <rdfs:comment>A tweet contains media content</rdfs:comment>
    <rdfs:domain rdf:resource="#Tweet"/>
    <rdfs:range rdf:resource="#Media"/>
    <owl:inverseOf rdf:resource="#attachedTo"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#attachedTo">
    <rdfs:label>attached to</rdfs:label>
    <rdfs:comment>Media content is attached to a tweet</rdfs:comment>
    <rdfs:domain rdf:resource="#Media"/>
    <rdfs:range rdf:resource="#Tweet"/>
</owl:ObjectProperty>

<!-- 
=================================================================
Data Properties - Attributes
=================================================================
-->

<owl:DatatypeProperty rdf:about="#username">
    <rdfs:label>username</rdfs:label>
    <rdfs:comment>The unique username of a user (e.g., @johndoe)</rdfs:comment>
    <rdfs:domain rdf:resource="#User"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:subPropertyOf rdf:resource="http://xmlns.com/foaf/0.1/nick"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="#displayName">
    <rdfs:label>display name</rdfs:label>
    <rdfs:comment>The display name of a user</rdfs:comment>
    <rdfs:domain rdf:resource="#User"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:subPropertyOf rdf:resource="http://xmlns.com/foaf/0.1/name"/>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="#bio">
    <rdfs:label>bio</rdfs:label>
    <rdfs:comment>The biography or description of a user</rdfs:comment>
    <rdfs:domain rdf:resource="#User"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="#tweetContent">
    <rdfs:label>tweet content</rdfs:label>
    <rdfs:comment>The textual content of a tweet (maximum 280 characters)</rdfs:comment>
    <rdfs:domain rdf:resource="#Tweet"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="#createdAt">
    <rdfs:label>created at</rdfs:label>
    <rdfs:comment>The timestamp when content was created</rdfs:comment>
    <rdfs:domain rdf:resource="#Content"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="#followerCount">
    <rdfs:label>follower count</rdfs:label>
    <rdfs:comment>The number of followers a user has</rdfs:comment>
    <rdfs:domain rdf:resource="#User"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"/>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="#followingCount">
    <rdfs:label>following count</rdfs:label>
    <rdfs:comment>The number of users a user is following</rdfs:comment>
    <rdfs:domain rdf:resource="#User"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"/>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="#likeCount">
    <rdfs:label>like count</rdfs:label>
    <rdfs:comment>The number of likes a tweet has received</rdfs:comment>
    <rdfs:domain rdf:resource="#Tweet"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"/>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="#retweetCount">
    <rdfs:label>retweet count</rdfs:label>
    <rdfs:comment>The number of retweets a tweet has received</rdfs:comment>
    <rdfs:domain rdf:resource="#Tweet"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"/>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="#replyCount">
    <rdfs:label>reply count</rdfs:label>
    <rdfs:comment>The number of replies a tweet has received</rdfs:comment>
    <rdfs:domain rdf:resource="#Tweet"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"/>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="#isVerified">
    <rdfs:label>is verified</rdfs:label>
    <rdfs:comment>Whether a user account is verified</rdfs:comment>
    <rdfs:domain rdf:resource="#User"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="#hashtagText">
    <rdfs:label>hashtag text</rdfs:label>
    <rdfs:comment>The text of a hashtag (without the # symbol)</rdfs:comment>
    <rdfs:domain rdf:resource="#Hashtag"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="#mediaUrl">
    <rdfs:label>media URL</rdfs:label>
    <rdfs:comment>The URL where media content can be accessed</rdfs:comment>
    <rdfs:domain rdf:resource="#Media"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#anyURI"/>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="#mediaType">
    <rdfs:label>media type</rdfs:label>
    <rdfs:comment>The MIME type of the media content</rdfs:comment>
    <rdfs:domain rdf:resource="#Media"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
</owl:DatatypeProperty>

<!-- 
=================================================================
Disjoint Class Declarations
=================================================================
-->

<owl:AllDisjointClasses>
    <owl:members rdf:parseType="Collection">
        <rdf:Description rdf:about="#Reply"/>
        <rdf:Description rdf:about="#Retweet"/>
        <rdf:Description rdf:about="#QuoteTweet"/>
    </owl:members>
</owl:AllDisjointClasses>

<owl:AllDisjointClasses>
    <owl:members rdf:parseType="Collection">
        <rdf:Description rdf:about="#Image"/>
        <rdf:Description rdf:about="#Video"/>
        <rdf:Description rdf:about="#Audio"/>
    </owl:members>
</owl:AllDisjointClasses>

