<?xml version="1.0"?>
<rdf:RDF xmlns="https://vityasyyy.github.io/ontologies/x-ontology.owl#"
     xml:base="https://vityasyyy.github.io/ontologies/x-ontology.owl"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:dc="http://purl.org/dc/elements/1.1/">
    
    <owl:Ontology rdf:about="https://vityasyyy.github.io/ontologies/x-ontology.owl">
        <owl:versionIRI rdf:resource="https://vityasyyy.github.io/ontologies/x-ontology.owl/1.0"/>
        <owl:imports rdf:resource="http://xmlns.com/foaf/0.1/"/>
        <owl:imports rdf:resource="http://rdfs.org/sioc/ns#"/>
        <dc:creator>Argya, Devin, Andrian</dc:creator>
        <dc:description>A Twitter ontology </dc:description>
        <dc:title>Twitter Social Media Ontology </dc:title>
    </owl:Ontology>
    
    
    <!-- Top-level abstract classes -->
    <owl:Class rdf:about="#TwitterEntity">
        <rdfs:label>Twitter Entity</rdfs:label>
        <rdfs:comment>Abstract base class for all Twitter entities</rdfs:comment>
    </owl:Class>
    
    <!-- USER CLASSES -->
    <owl:Class rdf:about="#User">
      <rdfs:subClassOf rdf:resource="http://xmlns.com/foaf/0.1/Person"/>
    </owl:Class> 

    <owl:Class rdf:about="#VerifiedUser">
        <rdfs:label>Verified User</rdfs:label>
        <rdfs:comment>A verified Twitter user account</rdfs:comment>
        <rdfs:subClassOf rdf:resource="#User"/>
    </owl:Class>
    
    <owl:Class rdf:about="#RegularUser">
        <rdfs:label>Regular User</rdfs:label>
        <rdfs:comment>A regular (non-verified) Twitter user account</rdfs:comment>
        <rdfs:subClassOf rdf:resource="#User"/>
        <owl:disjointWith rdf:resource="#VerifiedUser"/>
    </owl:Class>
    
    <owl:Class rdf:about="#Moderator">
        <rdfs:label>Moderator</rdfs:label>
        <rdfs:comment>A content moderator</rdfs:comment>
        <rdfs:subClassOf rdf:resource="#User"/>
    </owl:Class>
    
    <!-- CONTENT CLASSES -->
    <owl:Class rdf:about="#Content">
        <rdfs:label>Content</rdfs:label>
        <rdfs:comment>Abstract class for all content on Twitter</rdfs:comment>
        <rdfs:subClassOf rdf:resource="#TwitterEntity"/>
    </owl:Class>
    
    <owl:Class rdf:about="#Tweet">
        <rdfs:subClassOf rdf:resource="http://rdfs.org/sioc/ns#Post"/>
    </owl:Class>
    
    <owl:Class rdf:about="#OriginalTweet">
        <rdfs:label>Original Tweet</rdfs:label>
        <rdfs:comment>An original tweet (not a retweet or reply)</rdfs:comment>
        <rdfs:subClassOf rdf:resource="#Tweet"/>
    </owl:Class>
    
    <owl:Class rdf:about="#Retweet">
        <rdfs:label>Retweet</rdfs:label>
        <rdfs:comment>A retweet of another tweet</rdfs:comment>
        <rdfs:subClassOf rdf:resource="#Tweet"/>
        <owl:disjointWith rdf:resource="#OriginalTweet"/>
    </owl:Class>
    
    <owl:Class rdf:about="#Reply">
        <rdfs:label>Reply</rdfs:label>
        <rdfs:comment>A tweet that replies to another tweet</rdfs:comment>
        <rdfs:subClassOf rdf:resource="#Tweet"/>
    </owl:Class>
    
    <owl:Class rdf:about="#Advertisement">
        <rdfs:label>Advertisement</rdfs:label>
        <rdfs:comment>Sponsored content/advertisement</rdfs:comment>
        <rdfs:subClassOf rdf:resource="#Content"/>
        <owl:disjointWith rdf:resource="#Tweet"/>
    </owl:Class>
    
    <!-- MEDIA CLASSES -->
    <owl:Class rdf:about="#Media">
        <rdfs:label>Media</rdfs:label>
        <rdfs:comment>Media content attached to tweets</rdfs:comment>
        <rdfs:subClassOf rdf:resource="#TwitterEntity"/>
    </owl:Class>
    
    <owl:Class rdf:about="#Image">
        <rdfs:label>Image</rdfs:label>
        <rdfs:comment>Image media content</rdfs:comment>
        <rdfs:subClassOf rdf:resource="#Media"/>
    </owl:Class>
    
    <owl:Class rdf:about="#Video">
        <rdfs:label>Video</rdfs:label>
        <rdfs:comment>Video media content</rdfs:comment>
        <rdfs:subClassOf rdf:resource="#Media"/>
        <owl:disjointWith rdf:resource="#Image"/>
    </owl:Class>
    
    <owl:Class rdf:about="#GIF">
        <rdfs:label>GIF</rdfs:label>
        <rdfs:comment>Animated GIF media content</rdfs:comment>
        <rdfs:subClassOf rdf:resource="#Image"/>
    </owl:Class>
    
    <!-- HASHTAG CLASSES -->
    <owl:Class rdf:about="#Hashtag">
        <rdfs:label>Hashtag</rdfs:label>
        <rdfs:comment>A hashtag used for categorizing tweets</rdfs:comment>
        <rdfs:subClassOf rdf:resource="#TwitterEntity"/>
    </owl:Class>
    
    <owl:Class rdf:about="#TrendingHashtag">
        <rdfs:label>Trending Hashtag</rdfs:label>
        <rdfs:comment>A hashtag that is currently trending</rdfs:comment>
        <rdfs:subClassOf rdf:resource="#Hashtag"/>
    </owl:Class>
    
    <!-- TIMELINE CLASSES -->
    <owl:Class rdf:about="#Timeline">
        <rdfs:label>Timeline</rdfs:label>
        <rdfs:comment>A timeline showing tweets</rdfs:comment>
        <rdfs:subClassOf rdf:resource="#TwitterEntity"/>
    </owl:Class>
    
    <owl:Class rdf:about="#HomeTimeline">
        <rdfs:label>Home Timeline</rdfs:label>
        <rdfs:comment>A user's home timeline with tweets from followed users</rdfs:comment>
        <rdfs:subClassOf rdf:resource="#Timeline"/>
    </owl:Class>
    
    <owl:Class rdf:about="#UserTimeline">
        <rdfs:label>User Timeline</rdfs:label>
        <rdfs:comment>A specific user's tweet timeline</rdfs:comment>
        <rdfs:subClassOf rdf:resource="#Timeline"/>
    </owl:Class>
    
    <!-- 
    =================================================================
    Object Properties - Simple relationships
    =================================================================
    -->
    
    <owl:ObjectProperty rdf:about="#follows">
      <rdfs:subPropertyOf rdf:resource="http://xmlns.com/foaf/0.1/knows"/>
      <rdfs:label>follows</rdfs:label>
      <rdfs:comment>A user follows another user (specialization of foaf:knows)</rdfs:comment>
      <rdfs:domain rdf:resource="#User"/>
      <rdfs:range rdf:resource="#User"/>
      <owl:inverseOf rdf:resource="#followedBy"/>
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="#followedBy">
        <rdfs:label>followed by</rdfs:label>
        <rdfs:comment>A user is followed by another user</rdfs:comment>
        <rdfs:domain rdf:resource="#User"/>
        <rdfs:range rdf:resource="#User"/>
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="#posts">
        <rdfs:label>posts</rdfs:label>
        <rdfs:comment>A user posts a tweet</rdfs:comment>
        <rdfs:domain rdf:resource="#User"/>
        <rdfs:range rdf:resource="#Tweet"/>
        <owl:inverseOf rdf:resource="#postedBy"/>
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="#postedBy">
        <rdfs:label>posted by</rdfs:label>
        <rdfs:comment>A tweet is posted by a user</rdfs:comment>
        <rdfs:domain rdf:resource="#Tweet"/>
        <rdfs:range rdf:resource="#User"/>
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="#retweets">
        <rdfs:label>retweets</rdfs:label>
        <rdfs:comment>A tweet retweets another tweet</rdfs:comment>
        <rdfs:domain rdf:resource="#Retweet"/>
        <rdfs:range rdf:resource="#Tweet"/>
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="#repliesTo">
        <rdfs:label>replies to</rdfs:label>
        <rdfs:comment>A tweet replies to another tweet</rdfs:comment>
        <rdfs:domain rdf:resource="#Reply"/>
        <rdfs:range rdf:resource="#Tweet"/>
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="#likes">
        <rdfs:label>likes</rdfs:label>
        <rdfs:comment>A user likes a tweet</rdfs:comment>
        <rdfs:domain rdf:resource="#User"/>
        <rdfs:range rdf:resource="#Tweet"/>
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="#mentions">
        <rdfs:label>mentions</rdfs:label>
        <rdfs:comment>A tweet mentions a user</rdfs:comment>
        <rdfs:domain rdf:resource="#Tweet"/>
        <rdfs:range rdf:resource="#User"/>
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="#containsHashtag">
        <rdfs:label>contains hashtag</rdfs:label>
        <rdfs:comment>A tweet contains a hashtag</rdfs:comment>
        <rdfs:domain rdf:resource="#Tweet"/>
        <rdfs:range rdf:resource="#Hashtag"/>
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="#containsMedia">
        <rdfs:label>contains media</rdfs:label>
        <rdfs:comment>A tweet contains media content</rdfs:comment>
        <rdfs:domain rdf:resource="#Tweet"/>
        <rdfs:range rdf:resource="#Media"/>
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="#hasTimeline">
        <rdfs:label>has timeline</rdfs:label>
        <rdfs:comment>A user has a timeline</rdfs:comment>
        <rdfs:domain rdf:resource="#User"/>
        <rdfs:range rdf:resource="#Timeline"/>
    </owl:ObjectProperty>
    
    <!-- 
    =================================================================
    Data Properties - Simple attributes
    =================================================================
    -->
    
    <owl:DatatypeProperty rdf:about="#username">
        <rdfs:label>username</rdfs:label>
        <rdfs:comment>The unique username of a user</rdfs:comment>
        <rdfs:domain rdf:resource="#User"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>
    
    <owl:DatatypeProperty rdf:about="#displayName">
        <rdfs:label>display name</rdfs:label>
        <rdfs:comment>The display name of a user</rdfs:comment>
        <rdfs:domain rdf:resource="#User"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>
    
    <owl:DatatypeProperty rdf:about="#bio">
        <rdfs:label>bio</rdfs:label>
        <rdfs:comment>The biography/description of a user</rdfs:comment>
        <rdfs:domain rdf:resource="#User"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>
    
    <owl:DatatypeProperty rdf:about="#tweetContent">
        <rdfs:label>tweet content</rdfs:label>
        <rdfs:comment>The textual content of a tweet</rdfs:comment>
        <rdfs:domain rdf:resource="#Tweet"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>
    
    <owl:DatatypeProperty rdf:about="#createdAt">
        <rdfs:label>created at</rdfs:label>
        <rdfs:comment>The timestamp when content was created</rdfs:comment>
        <rdfs:domain rdf:resource="#Content"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
    </owl:DatatypeProperty>
    
    <owl:DatatypeProperty rdf:about="#followerCount">
        <rdfs:label>follower count</rdfs:label>
        <rdfs:comment>The number of followers a user has</rdfs:comment>
        <rdfs:domain rdf:resource="#User"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"/>
    </owl:DatatypeProperty>
    
    <owl:DatatypeProperty rdf:about="#followingCount">
        <rdfs:label>following count</rdfs:label>
        <rdfs:comment>The number of users a user is following</rdfs:comment>
        <rdfs:domain rdf:resource="#User"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"/>
    </owl:DatatypeProperty>
    
    <owl:DatatypeProperty rdf:about="#likeCount">
        <rdfs:label>like count</rdfs:label>
        <rdfs:comment>The number of likes a tweet has received</rdfs:comment>
        <rdfs:domain rdf:resource="#Tweet"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"/>
    </owl:DatatypeProperty>
    
    <owl:DatatypeProperty rdf:about="#retweetCount">
        <rdfs:label>retweet count</rdfs:label>
        <rdfs:comment>The number of retweets a tweet has received</rdfs:comment>
        <rdfs:domain rdf:resource="#Tweet"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"/>
    </owl:DatatypeProperty>
    
    <owl:DatatypeProperty rdf:about="#isVerified">
        <rdfs:label>is verified</rdfs:label>
        <rdfs:comment>Whether a user account is verified</rdfs:comment>
        <rdfs:domain rdf:resource="#User"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
    </owl:DatatypeProperty>
    
    <owl:DatatypeProperty rdf:about="#hashtagText">
        <rdfs:label>hashtag text</rdfs:label>
        <rdfs:comment>The text of a hashtag</rdfs:comment>
        <rdfs:domain rdf:resource="#Hashtag"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>
    
    <!-- 
    =================================================================
    Sample Individuals
    =================================================================
    -->
    
    <VerifiedUser rdf:about="#user_elonmusk">
        <username>elonmusk</username>
        <displayName>Elon Musk</displayName>
        <bio>CEO of Tesla and SpaceX</bio>
        <followerCount rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">150000000</followerCount>
        <followingCount rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">500</followingCount>
        <isVerified rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</isVerified>
    </VerifiedUser>
    
    <RegularUser rdf:about="#user_johnsmith">
        <username>johnsmith</username>
        <displayName>John Smith</displayName>
        <bio>Software developer and tech enthusiast</bio>
        <followerCount rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1250</followerCount>
        <followingCount rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">890</followingCount>
        <isVerified rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">false</isVerified>
    </RegularUser>
    
    <OriginalTweet rdf:about="#tweet001">
        <tweetContent>Just launched another rocket to Mars! ðŸš€ #SpaceX #Mars</tweetContent>
        <postedBy rdf:resource="#user_elonmusk"/>
        <likeCount rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">89000</likeCount>
        <retweetCount rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">25000</retweetCount>
        <createdAt rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2025-09-23T10:30:00Z</createdAt>
    </OriginalTweet>
    
    <Reply rdf:about="#tweet002">
        <tweetContent>That's incredible! When is the first human mission?</tweetContent>
        <postedBy rdf:resource="#user_johnsmith"/>
        <repliesTo rdf:resource="#tweet001"/>
        <likeCount rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">245</likeCount>
        <retweetCount rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">12</retweetCount>
        <createdAt rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2025-09-23T11:15:00Z</createdAt>
    </Reply>
    
    <Hashtag rdf:about="#hashtag_spacex">
        <hashtagText>SpaceX</hashtagText>
    </Hashtag>
    
    <Hashtag rdf:about="#hashtag_mars">
        <hashtagText>Mars</hashtagText>
    </Hashtag>
    
    <!-- Relations -->
    <rdf:Description rdf:about="#user_johnsmith">
        <follows rdf:resource="#user_elonmusk"/>
        <likes rdf:resource="#tweet001"/>
    </rdf:Description>
    
    <rdf:Description rdf:about="#tweet001">
        <containsHashtag rdf:resource="#hashtag_spacex"/>
        <containsHashtag rdf:resource="#hashtag_mars"/>
    </rdf:Description>
    
</rdf:RDF>
